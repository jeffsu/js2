exports.apply = function (root) {
  <%= js 'core/js2-boot.js' %>

  var JS2 = root.JS2 = mainFunction;
  var js2 = root.js2 = JS2;
  js2.ROOT = JS2;

  <%= core %>

  <%= js 'Common/NodeFileAdapter.js2' %>
  JS2.fs = new JS2.FileSystem(new JS2.NodeFileAdapter());

  js2.ROOT = root;
  js2.DECORATOR = new JS2.Decorator.Node();
  return js2;
};
